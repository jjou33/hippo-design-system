{
  "name": "hippods",
  "version": "0.0.0",
  "scripts": {
    "dev": "turbo dev",
    "test": "pnpm build && pnpm dev",
    "build": "turbo build",
    "serve": "turbo build:start",
    "dev:all": "pnpm dev:packages --parallel && pnpm dev:storybook",
    "dev:storybook": "pnpm --filter @hippods/storybook storybook",
    "dev:packages": "pnpm --filter @hippods/react-components-* dev"
  },
  "devDependencies": {
    "turbo": "^2.2.3",
    "@storybook/addon-docs": "^7.4.0",
    "@typescript-eslint/eslint-plugin": "^6.5.0",
    "@typescript-eslint/parser": "^6.5.0",
    "eslint": "^8.48.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-json-format": "^2.0.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-storybook": "^0.6.13",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2",
    "tailwindcss": "^3.4.1"
  },
  "packageManager": "pnpm@8.7.1+sha256.9f0febfd67c8f4317f4cf2edbf2ff4bf326f0e7dbb2b5b03aa05248f9faa26c5"
